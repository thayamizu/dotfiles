{
    //Windows
    "window.openFilesInNewWindow": "on",
    //Icons
    "vsicons.presets.hideFolders": false,
    //Files
    "files.associations": {
        "*.log": "txt",
        "*.blade": "blade"
    },
    //Editor
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.find.addExtraSpaceOnTop": false,
    "editor.fontFamily": "'Cascadia Code', 'IPA Pゴシック', Consolas, 'Courier New', monospace",
    "editor.fontSize": 16,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "editor.suggestSelection": "first",
    "editor.insertSpaces": true,
    "editor.detectIndentation": true,
    "editor.fontWeight": "normal",
    //workbench
    "workbench.colorCustomizations": {
        "terminal.foreground": "#bebfc9",
        "terminal.background": "#23262b"
    },
    "workbench.fontAliasing": "auto",
    "workbench.editor.enablePreview": false,
    "workbench.colorTheme": "Default Light+",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.activityBar.visible": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "material-icon-theme.folders.theme": "specific",
    "material-icon-theme.folders.color": "#fdd835",
    "vsicons.dontShowNewVersionMessage": true,
    //Terminal
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.fontWeight": "normal",
    "terminal.integrated.fontFamily": "'Cascadia Code', 'IPA Pゴシック'",
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    "terminal.integrated.shell.osx": "zsh",
    //DebugConsole
    "debug.console.fontSize": 14,
    //SCM
    "scm.inputFontSize": 14,
    //Explorer
    "explorer.confirmDragAndDrop": false,
    //Git
    "git.confirmSync": false,
    "git.autofetch": true,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.defaultGravatarsStyle": "robohash",
    "gitlens.keymap": "alternate",
    "gitlens.views.lineHistory.enabled": true,
    //Emmet
    "emmet.includeLanguages": {
        "vue-html": "html",
        "vue": "vue",
        "javascript": "javascriptreact",
        "typescript": "typescriptreact"
    },
    "emmet.syntaxProfiles": {
        "javascript": "jsx",
        "typescript": "tsx"
    },
    //Langauge
    "C_Cpp.updateChannel": "Insiders",
    "C_Cpp.workspaceSymbols": "All",
    "go.toolsManagement.autoUpdate": true,
    "omnisharp.useGlobalMono": "never",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "php.validate.enable": false, //組み込みのvalidaionはオフにしておく
    "php.validate.executablePath": "/usr/local/bin/php",
    "php.validate.run": "onType",
    "php-cs-fixer.lastDownload": 1629938613858,
    "php-cs-fixer.executablePath": "${extensionPath}/php-cs-fixer.phar",
    "intelephense.diagnostics.undefinedTypes": false,
    "intelephense.diagnostics.undefinedFunctions": false,
    "intelephense.diagnostics.undefinedConstants": false,
    "intelephense.diagnostics.undefinedClassConstants": false,
    "intelephense.diagnostics.undefinedMethods": false,
    "intelephense.diagnostics.undefinedProperties": false,
    "intelephense.diagnostics.undefinedVariables": false,
    "intelephense.diagnostics.run": "onType",
    "python.languageServer": "Microsoft",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.implementationsCodeLens.enabled": true,
    "typescript.referencesCodeLens.enabled": true,
    "typescript.tsdk": "/usr/local/lib/node_modules/typescript/lib",
    //LiveShare
    "liveshare.featureSet": "insiders",
    //AWS
    "aws.profile": "profile:default",
    //WorkspaceExplorer
    "workspaceExplorer.enableCustomIconSearch": true,
    "workspaceExplorer.workspaceStorageDirectory": "~/Documents/projects",
    ///CodeRunner
    "code-runner.clearPreviousOutput": true,
    "code-runner.defaultLanguage": "c",
    //Peacock
    "peacock.favoriteColors": [
        {
            "name": "Angular Red",
            "value": "#b52e31"
        },
        {
            "name": "Auth0 Orange",
            "value": "#eb5424"
        },
        {
            "name": "Azure Blue",
            "value": "#007fff"
        },
        {
            "name": "C# Purple",
            "value": "#68217A"
        },
        {
            "name": "Gatsby Purple",
            "value": "#639"
        },
        {
            "name": "Go Cyan",
            "value": "#5dc9e2"
        },
        {
            "name": "Java Blue-Gray",
            "value": "#557c9b"
        },
        {
            "name": "JavaScript Yellow",
            "value": "#f9e64f"
        },
        {
            "name": "Mandalorian Blue",
            "value": "#1857a4"
        },
        {
            "name": "Node Green",
            "value": "#215732"
        },
        {
            "name": "React Blue",
            "value": "#00b3e6"
        },
        {
            "name": "Something Different",
            "value": "#832561"
        },
        {
            "name": "Vue Green",
            "value": "#42b883"
        }
    ],
    //Java
    "java.semanticHighlighting.enabled": true,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    //IntelliCode
    "vsintellicode.features.python.deepLearning": "enabled",
    "http.proxySupport": "off",
    "gitlens.mode.active": "zen",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "yaml.schemas": {},
}
