{
    //Windows
    "window.openFilesInNewWindow": "on",
    //Icons
    "vsicons.presets.hideFolders": false,
    //Files
    "files.associations": {
        "*.log": "txt",
        "*.blade": "blade"
    },
    //Editor
    "editor.find.addExtraSpaceOnTop": false,
    "editor.fontFamily": "'Inconsolata-dz for Powerline','IPA Pゴシック', Consolas, 'Courier New', monospace",
    "editor.fontSize": 16,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "editor.suggestSelection": "first",
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.detectIndentation": true,
    "editor.fontWeight": "bold",
    "editor.lineNumbers": "relative",
    //Workbench
    "workbench.colorTheme": "One Dark Pro",
    "workbench.colorCustomizations": {
        "terminal.foreground": "#bebfc9",
        "terminal.background": "#23262b"
    },
    "workbench.editor.enablePreview": false,
    "workbench.iconTheme": "material-icon-theme",
    //Terminal
    "terminal.integrated.fontFamily": "'Inconsolata-dz for Powerline', 'IPA Pゴシック'",
    "terminal.integrated.shell.windows": "wsl",
    "terminal.integrated.shell.osx": "zsh",
    //Explorer
    "explorer.confirmDragAndDrop": false,
    //Git
    "git.confirmSync": false,
    "git.autofetch": true,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.defaultGravatarsStyle": "robohash",
    "gitlens.keymap": "alternate",
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,
    //Emmet
    "emmet.includeLanguages": {
        "vue-html": "html",
        "vue": "vue",
        "javascript": "javascriptreact",
        "typescript": "typescriptreact"
    },
    "emmet.syntaxProfiles": {
        "javascript": "jsx",
        "typescript": "tsx"
    },
    //Langauge
    "C_Cpp.updateChannel": "Insiders",
    "C_Cpp.workspaceSymbols": "All",
    "python.jediEnabled": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "latex-workshop.view.pdf.viewer": "tab",
    "php.validate.executablePath": "/usr/local/bin/php",
    "php.validate.run": "onType",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.implementationsCodeLens.enabled": true,
    "typescript.referencesCodeLens.enabled": true,
    "typescript.tsdk": "/usr/local/lib/node_modules/typescript/lib",
    //LiveShare
    "liveshare.featureSet": "insiders",
    //AWS
    "aws.profile": "default",
    //WorkspaceExplorer
    "workspaceExplorer.enableCustomIconSearch": true,
    "workspaceExplorer.workspaceStorageDirectory": "/path/to/workspace",
    ///CodeRunner
    "code-runner.clearPreviousOutput": true,
    "code-runner.defaultLanguage": "c",
    //Peacock
    "peacock.favoriteColors": [
        {
            "name": "Angular Red",
            "value": "#b52e31"
        },
        {
            "name": "Auth0 Orange",
            "value": "#eb5424"
        },
        {
            "name": "Azure Blue",
            "value": "#007fff"
        },
        {
            "name": "C# Purple",
            "value": "#68217A"
        },
        {
            "name": "Gatsby Purple",
            "value": "#639"
        },
        {
            "name": "Go Cyan",
            "value": "#5dc9e2"
        },
        {
            "name": "Java Blue-Gray",
            "value": "#557c9b"
        },
        {
            "name": "JavaScript Yellow",
            "value": "#f9e64f"
        },
        {
            "name": "Mandalorian Blue",
            "value": "#1857a4"
        },
        {
            "name": "Node Green",
            "value": "#215732"
        },
        {
            "name": "React Blue",
            "value": "#00b3e6"
        },
        {
            "name": "Something Different",
            "value": "#832561"
        },
        {
            "name": "Vue Green",
            "value": "#42b883"
        }
    ],
    //Vim
    "vim.easymotion": true,
    "vim.hlsearch": true,
    "vim.ignorecase": true,
    "vim.incsearch": true,
    "vim.sneak": false,
    "vim.useCtrlKeys": true,
    "vim.useSystemClipboard": true,
    "vim.visualstar": true,
    "vim.camelCaseMotion.enable": false,
    "vim.highlightedyank.enable": true,
    "vim.history": 100,
    "vim.leader": " ",
    "vim.statusBarColorControl": false,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "f"
            ],
            "after": [
                "<leader>",
                "<leader>",
                "s"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        }
    ],
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    "material-icon-theme.folders.theme": "specific",
    "material-icon-theme.folders.color": "#fdd835"
}
